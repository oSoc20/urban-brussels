}\r\n\r\n.building__info {\r\n  max-width: 330px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.building-list__item {\r\n  cursor: pointer;\r\n  border-bottom: 1px solid rgba(44, 53, 80, .1);\r\n  box-shadow: inset 0 0 0 rgba(44, 53, 80, .03);\r\n  transition: all .4s ease-in;\r\n  min-width: 490px;\r\n}\r\n\r\n.building-list__item:hover {\r\n  box-shadow: inset 530px 0 0 rgba(44, 53, 80, .03)\r\n}\r\n\r\n\r\n\r\n.building-list__item__container, .building__tags {\r\n  display: flex;\r\n}\r\n\r\n.building-list__item__container {\r\n  padding: 0 25px;\r\n}\r\n\r\n.building__tags {\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.building-list__item__container {\r\n  margin: 30px 0;\r\n}\r\n\r\n.building__img {\r\n  border-radius: 15px;\r\n  width: 115px;\r\n  margin-right: 30px;\r\n  background-size: cover;\r\n  background-repeat: no-repeat;\r\n  min-height: 90px;\r\n}\r\n\r\n.building__name, .building__street, .building__municipality {\r\n  color: #212E44;\r\n  font-size: 16px;\r\n  font-weight: 700;  \r\n  line-height: 25px;\r\n}\r\n\r\n.building__name {\r\n  font-weight: 900;\r\n  font-size: 17px;\r\n  margin-bottom: 7px;\r\n}\r\n\r\n\r\n.pop-up {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.pop-up__address {\r\n  display: flex;\r\n  flex-direction: column;\r\n  margin-right: 5px;\r\n}\r\n\r\n.pop-up__img {\r\n  object-fit: cover;\r\n  width: 60px;\r\n  height: 60px;\r\n  border-radius: 10px;\r\n  margin-right: 15px;\r\n}\r\n\r\n.pop-up__info {\r\n  color: #212E44;\r\n  font-size: 14px;\r\n  font-weight: 600;\r\n  word-wrap: break-word;\r\n}\r\n\r\n.switch {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: #E9F7F3;\r\n  border-radius: 10px;\r\n  position: fixed;\r\n  z-index: 3;\r\n  padding: 7px 15px 7px 25px;\r\n  right: 40px;\r\n  top: 40px;\r\n  box-shadow: 0px 3.3655px 3.3655px rgba(0, 0, 0, 0.04);\r\n}\r\n\r\n.switch p {\r\n  font-size: 12px;\r\n  font-weight: 700;\r\n  color: #2C3550;\r\n}\r\n\r\ninput[type=checkbox]{\r\n\theight: 0;\r\n\twidth: 0;\r\n\tvisibility: hidden;\r\n}\r\n\r\nlabel {\r\n\tcursor: pointer;\r\n\ttext-indent: -9999px;\r\n  width: 60px;\r\n  height: 30px;\r\n\tbackground: #C1C5D3;\r\n\tdisplay: block;\r\n\tborder-radius: 100px;\r\n  position: relative;\r\n  margin-left: 10px;\r\n\r\n}\r\n\r\nlabel:after {\r\n\tcontent: '';\r\n\tposition: absolute;\r\n  top: 3px;\r\n  left: 5px;\r\n  width: 24px;\r\n  height: 24px;\r\n\tbackground: #fff;\r\n\tborder-radius: 90px;\r\n\ttransition: 0.3s;\r\n}\r\n\r\ninput:checked + label {\r\n\tbackground: #81D8BF;\r\n}\r\n\r\ninput:checked + label:after {\r\n\tleft: calc(100% - 5px);\r\n\ttransform: translateX(-100%);\r\n}\r\n\r\nlabel:active:after {\r\n\twidth: 45px;\r\n}\r\n\r\n.pagination {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.arrowLeft, .arrowRight {\r\n  cursor: pointer;\r\n}\r\n\r\n.arrowLeft img, .arrowRight img {\r\n  width: 14px;\r\n}\r\n\r\n.arrowLeft {\r\n  transform: rotate(180deg);\r\n  margin-bottom: 2px;\r\n}\r\n\r\n.pagNumber, .pagDots{\r\n  display: none;\r\n}\r\n\r\n.pagination__page {\r\n  font-weight: 700;\r\n  font-size: 16px;\r\n  color: #81D8BF;\r\n  line-height: 0;\r\n  margin: 0 10px;\r\n  padding-bottom: 2px;\r\n}\r\n\r\n.is-hidden {\r\n  visibility: hidden;\r\n}\r\n\r\n.is-not-visible {\r\n  display: none;\r\n}\r\n\r\n.search-bar {\r\n  margin-bottom: 10px\r\n}\r\n\r\n.search-bar__input {\r\n  border: none;\r\n  background: #F5F5F5;\r\n  border-radius: 10px;\r\n  height: 35px;\r\n  width: 100%;\r\n  color: #333B56;\r\n  padding: 10px;\r\n  font-size: 15px;\r\n  font-weight: 500;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":260}},"error":null,"hash":"74242e8c5c53062b0223b1024428864d","cacheData":{"env":{}}}